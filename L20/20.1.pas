PROGRAM PseudoGraph(INPUT, OUTPUT);
VAR
  Ch: CHAR;
PROCEDURE PrintSymbol(VAR Ch: CHAR);
VAR
  Code: SET OF 1..25;
  Count: INTEGER; 
BEGIN {PrintSymbol}
  Code := [];
  CASE Letter OF
    'A', 'a': Code := [3, 7, 9, 11, 15, 16, 17, 18, 19, 20, 21, 25];
    'B', 'b': Code := [1, 2, 3, 4, 6, 9, 11, 12, 13, 14, 15, 16, 20, 21, 22, 23, 24, 25];
    'C', 'c': Code := [1, 2, 3, 4, 5, 6, 10, 11, 16, 20, 21, 22, 23, 24, 25];
    'D', 'd': Code := [1, 2, 3, 6, 9, 11, 15, 16, 20, 21, 22, 23, 24, 25];
    'E', 'e': Code := [1, 2, 3, 4, 5, 6, 11, 12, 13, 16, 21, 22, 23, 24, 25];
    'F', 'f': Code := [1, 2, 3, 4, 5, 6, 11, 16, 17, 18, 21];
    'G', 'g': Code := [2, 3, 4, 6, 11, 13, 14, 15, 16, 20, 21, 22, 23, 24, 25];
    'H', 'h': Code := [1, 5, 6, 10, 11, 15, 16, 17, 18, 19, 20, 21, 25];
    'I', 'i': Code := [2, 3, 4, 8, 13, 18, 22, 23, 24];
    'J', 'j': Code := [2, 3, 4, 9, 14, 16, 19, 22, 23];
    'K', 'k': Code := [1, 5, 6, 9, 11, 12, 13, 16, 19, 21, 25];
    'L', 'l': Code := [1, 6, 11, 16, 20, 21, 22, 23, 24, 25];
    'M', 'm': Code := [1, 2, 4, 5, 6, 8, 10, 11, 15, 16, 20, 21, 25];
    'N', 'n': Code := [1, 2, 5, 6, 8, 10, 11, 14, 15, 16, 22, 21, 25];
    'O', 'o': Code := [2, 3, 4, 6, 9, 11, 15, 16, 20, 22, 23, 24, 25];
    'P', 'p': Code := [1, 2, 3, 4, 6, 10, 11, 15, 16, 17, 18, 19, 20, 21];
    'Q', 'q': Code := [2, 3, 4, 6, 10, 11, 13, 15, 16, 19, 20, 22, 23, 24, 25];
    'R', 'r': Code := [1, 2, 3, 4, 6, 10, 11, 12, 13, 14, 15, 16, 19, 21, 25];
    'S', 's': Code := [3, 4, 5, 7, 13, 19, 21, 22, 23];
    'T', 't': Code := [1, 2, 3, 4, 5, 6, 8, 10, 13, 18, 23];
    'U', 'u': Code := [1, 5, 6, 10, 11, 15, 16, 20, 22, 23, 24];
    'V', 'v': Code := [1, 5, 6, 10, 11, 15, 17, 19, 23];
    'W', 'w': Code := [1, 5, 6, 10, 11, 15, 16, 18, 20, 22, 24];
    'X', 'x': Code := [2, 4, 8, 12, 14, 16, 20, 21, 25];
    'Y', 'y': Code := [1, 5, 6, 10, 12, 14, 18, 23];
    'Z', 'z': Code := [2, 3, 4, 5, 9, 13, 17, 22, 23, 24, 25]
  END;
  IF Code <> []
  THEN
    BEGIN
      FOR Count := 1 TO 25
      DO
        BEGIN
          IF Count IN Code
          THEN
            WRITE('@')
          ELSE
            WRITE(' ');
          IF Count MOD 5 = 0
          THEN
            WRITELN
        END;
      WRITELN        
    END
  ELSE
    WRITELN    
END; {PrintSymbol}
BEGIN {PseudoGraph}
  WHILE NOT EOLN
  DO
    BEGIN
      READ(Ch);
      WriteGraphicPrinting(Ch)
    END
END. {PseudoGraph}
